<Page
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="NGrabber.Page1"
	x:Name="Page"
	WindowTitle="Page"
	FlowDirection="LeftToRight"
	Width="640" Height="480"
	WindowWidth="640" WindowHeight="480">

	<Grid x:Name="LayoutRoot">
		<Ellipse HorizontalAlignment="Left" Margin="100,100,0,0" Width="100" Fill="#FF45980F" VerticalAlignment="Top" Height="100"/>
		<Ellipse HorizontalAlignment="Left" Margin="100,100,0,0" VerticalAlignment="Top" Width="100" Height="100" Fill="{DynamicResource statusDisk}"/>
	</Grid>
</Page>